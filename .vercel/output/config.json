{
	"version": 3,
	"routes": [
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/?(?:/__data.json)?$",
			"dest": "/"
		},
		{
			"src": "^/\\.well-known/mcp\\.json/?(?:/__data.json)?$",
			"dest": "/.well-known/mcp.json"
		},
		{
			"src": "^/about/?(?:/__data.json)?$",
			"dest": "/about"
		},
		{
			"src": "^/api/mcp/?(?:/__data.json)?$",
			"dest": "/api/mcp"
		},
		{
			"src": "^/api/mcp/invoke/?(?:/__data.json)?$",
			"dest": "/api/mcp/invoke"
		},
		{
			"src": "^/api/mcp/tools/?(?:/__data.json)?$",
			"dest": "/api/mcp/tools"
		},
		{
			"src": "^/api/reactions/([^/]+?)/?(?:/__data.json)?$",
			"dest": "/api/reactions/[slug]"
		},
		{
			"src": "^/blog/?(?:/__data.json)?$",
			"dest": "/blog"
		},
		{
			"src": "^/blog/([^/]+?)/?(?:/__data.json)?$",
			"dest": "/blog/[slug]"
		},
		{
			"src": "^/projects/?(?:/__data.json)?$",
			"dest": "/projects"
		},
		{
			"src": "^/tools/?(?:/__data.json)?$",
			"dest": "/tools"
		},
		{
			"src": "^/tools/base64/?(?:/__data.json)?$",
			"dest": "/tools/base64"
		},
		{
			"src": "^/tools/code-explainer/?(?:/__data.json)?$",
			"dest": "/tools/code-explainer"
		},
		{
			"src": "^/tools/code-simplifier/?(?:/__data.json)?$",
			"dest": "/tools/code-simplifier"
		},
		{
			"src": "^/tools/json-formatter/?(?:/__data.json)?$",
			"dest": "/tools/json-formatter"
		},
		{
			"src": "^/tools/prompt-optimizer/?(?:/__data.json)?$",
			"dest": "/tools/prompt-optimizer"
		},
		{
			"src": "^/tools/regex-tester/?(?:/__data.json)?$",
			"dest": "/tools/regex-tester"
		},
		{
			"src": "^/tools/timestamp/?(?:/__data.json)?$",
			"dest": "/tools/timestamp"
		},
		{
			"src": "^/tools/token-calculator/?(?:/__data.json)?$",
			"dest": "/tools/token-calculator"
		},
		{
			"src": "/.*",
			"dest": "/![-]/catchall"
		}
	],
	"overrides": {}
}